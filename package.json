{
    "name": "cyphers-discord-bot",
    "version": "1.1.0",
    "main": "dist/index.js",
    "author": "Insiro",
    "license": "MIT",
    "private": true,
    "scripts": {
        "tsc": "tsc",
        "build": "yarn tsc -p tsconfig.json",
        "lint": "yarn eslint \"src/**/*\"",
        "lint-fix": "yarn lint --fix",
        "start": "ts-node src/index.ts",
        "start:dev": "yarn nodemon --watch src --ext \"ts tsx\" --delay 100ms --exec \"ts-node src/index.ts\"",
        "start:prod": "yarn node .",
        "makemigration": "yarn typeorm migration:generate -n",
        "migrates": "yarn typeorm migration:run",
        "migrate:rollback": "yarn typeorm migration:revert"
    },
    "dependencies": {
        "@types/cheerio": "^0.22.18",
        "@types/jest": "^26.0.0",
        "@types/node": "^14.0.14",
        "@types/node-schedule": "^1.3.0",
        "@types/pg": "^7.14.3",
        "@types/request": "^2.48.5",
        "@types/request-promise-native": "^1.0.17",
        "@types/ws": "^7.2.5",
        "@types/xml2js": "^0.4.5",
        "@typescript-eslint/eslint-plugin": "^2.14.0",
        "@typescript-eslint/parser": "^3.4.0",
        "cheerio": "^1.0.0-rc.3",
        "class-validator": "^0.11.0",
        "cross-env": "^7.0.2",
        "discord-command-parser": "^1.4.3",
        "discord.js": "^12.2.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.2.0",
        "eslint-plugin-jest": "^23.13.2",
        "jest": "^26.0.1",
        "node-schedule": "^1.3.2",
        "nodemon": "^2.0.4",
        "pg": "^8.2.1",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.2",
        "request-promise-native": "^1.0.8",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.10.2",
        "typeorm": "0.2.25",
        "typescript": "^3.7.4",
        "ws": "^7.3.0",
        "xml2js": "^0.4.23"
    }
}
